{
  "name": "@eonian/web",
  "version": "0.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -c \"auto\" \"npm:dev:next\" \"npm:check:ts:d-watch\"",
    "dev:next": "next dev",
    "dev:no-tls": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "dev:mock": "NEXT_PUBLIC_BSC_GRAPH_URL=http://localhost:4004/graphql NEXT_PUBLIC_WALLET_LINKING_URL=http://localhost:5000/ concurrently -c \"auto\"  \"npm:dev:next\" \"npm:check:ts:d-watch\" \"npm:mock:protocol\" \"npm:mock:wallet-linking\" ",
    "build": "next build --no-lint",
    "postbuild": "npm run release-logrocket",
    "start": "next start",
    "lint": "next lint",
    "format": "next lint --fix",
    "storybook": "storybook dev -p 6006 --quiet --no-open",
    "build:storybook": "storybook build",
    "serve:storybook": "serve ./storybook-static",
    "production": "next build && next start",
    "open:next": "opener http://localhost:3000",
    "test": "npm run jest",
    "test:watch": "npm run jest -- --watchAll",
    "test:ci": "npm run jest --ci",
    "jest": "node --experimental-vm-modules --experimental-specifier-resolution=node ./node_modules/.bin/jest",
    "mock": "concurrently -c \"auto\" \"npm:mock:protocol\" \"npm:mock:wallet-linking\"",
    "mock:protocol": "tsx ./graphql/mocks/protocol-mock.ts",
    "mock:wallet-linking": "tsx ./graphql/mocks/wallet-linking-mock.ts",
    "gen:gql-types": "npm run gen:gql:protocol && npm run gen:gql:wallet-linking",
    "gen:gql:protocol": "graphql-codegen --config ./graphql/protocol/codegen.ts",
    "gen:gql:wallet-linking": "graphql-codegen --config ./graphql/wallet-linking/codegen.ts",
    "release-logrocket": "node release-logrocket.cjs",
    "postinstall": "cp .hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "check:ts": "tsc --noEmit --incremental",
    "check:ts:d-watch": "sleep 5s && npm run check:ts:watch",
    "check:ts:watch": "npm run check:ts -- --watch --preserveWatchOutput"
  },
  "engines": {
    "node": ">=18.17.1 <19.0.0"
  },
  "dependencies": {
    "@apollo/client": "3.10.4",
    "@apollo/experimental-nextjs-app-support": "0.10.1",
    "@auth0/auth0-react": "^2.1.1",
    "@mantine/core": "^7.12.1",
    "@mantine/hooks": "^7.12.1",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next/mdx": "13.5.6",
    "@nextui-org/react": "2.2.9",
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgdotjs/svg.js": "^3.1.2",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.9",
    "@types/react-transition-group": "^4.4.5",
    "@uidotdev/usehooks": "^2.4.1",
    "@web3-onboard/core": "^2.22.1",
    "@web3-onboard/injected-wallets": "^2.11.1",
    "@web3-onboard/react": "^2.9.1",
    "@web3-onboard/walletconnect": "^2.6.1",
    "clsx": "^1.2.1",
    "ethers": "^6.5.1",
    "framer-motion": "10.16.16",
    "graphql": "^16.8.1",
    "lodash.debounce": "^4.0.8",
    "logrocket": "^5.0.1",
    "luxon": "^3.5.0",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-content-loader": "^7.0.2",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.48.2",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.3",
    "react-transition-group": "^4.4.5",
    "react-type-animation": "^3.2.0",
    "react-wrap-balancer": "^0.4.0",
    "recharts": "^2.12.7",
    "redux": "^4.2.1",
    "siwe": "^2.3.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.3",
    "@eonian/eslint-config": "^0.0.7",
    "@eonian/graphql-typescript-scalar-type-policies": "^1.0.6",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "3.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-resolvers": "^3.2.0",
    "@graphql-tools/graphql-file-loader": "^7.5.16",
    "@graphql-tools/load": "^7.8.13",
    "@graphql-tools/mock": "^8.7.19",
    "@graphql-tools/schema": "^9.0.17",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/nextjs": "7.5.3",
    "@storybook/react": "7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/lodash.debounce": "^4.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.9.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "autoprefixer": "^10.4.16",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "eslint": "^8.48.0",
    "eslint-config-next": "^14.2.4",
    "eslint-plugin-storybook": "^0.6.13",
    "express": "^4.19.2",
    "graphql-scalars": "^1.21.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "logrocket-cli": "^0.13.1",
    "opener": "^1.5.2",
    "postcss": "^8.4.41",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "react-tweet": "^3.2.1",
    "rehype-external-links": "^2.0.1",
    "sass": "^1.60.0",
    "serve": "^14.2.0",
    "storybook": "7.4.0",
    "storybook-addon-pseudo-states": "^2.0.0-next.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "tsx": "^4.17.0",
    "webpack-dev-middleware": "^6.1.2"
  },
  "overrides": {
    "chokidar": {
      "braces": "^3.0.3"
    },
    "micromatch": {
      "braces": "^3.0.3"
    }
  }
}
